set -e
export JAVA_VERSION=11
distroFolder="$(pwd)"
cd ../net.splitcells.network.bom
  mvn clean install
cd ../net.splitcells.network
  mvn clean install
cd ../net.splitcells.network.media
  mvn clean install
cd ../net.splitcells.network.log
  mvn clean install
cd "$distroFolder"
mvn clean install
